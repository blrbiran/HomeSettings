bbrmyocbuild() { mkdir old && mv build/ sstate-cache/ old ; \
	echo "start rm old folder" && rm -rf old ; }
bbpush() { git push mediatek HEAD:refs/for/$1 ; }
bbcl() { cas-labels -b alps-mp-$1 ; }
bbct() { s1=( $(sed -r "s/(alps)-(mp|dev|trunk)-([0-9a-z.]*)-/\3-$2-/g" <<< "$1"));
cas-take $1 -n $s1 ;
}
bbct2() {
s0=( $(grep --color=auto -Po "(?<=alps-mp-)[a-zA-Z0-9.\-_]*" <<< "$1") ) ;
s1=( $(grep --color=auto -Eo "[a-zA-Z0-9.]*-" <<< "$s0" ) ) ;
s2=$s1$2-${s0:${#s1}:${#s0}} ; cas-take $1 -n $s2 ; }
bbct3() { cas-take alps-mp-$1 -n $2 ; }
bbmeld() { meld $1/$3 $2/$3 ; }
bbmeldc() { cur=${PWD} ;
if [ "$2" == "" ];then
	meld $cur/ $1/ ;
else
	meld $cur/$2 $1/$2 ;
fi ; }
bbup() { ls $* | xargs -I {} mtkbuild -o . -l -i -d -x {} ; }
bbv() { var=$*;file=${var%%:*};tmp=${var#*:};line=${tmp%%:*};
	vi_para=$file" +"$line;echo $vi_para; vi $vi_para ; }
bb2ack() { ack "$1" -l | xargs -I {} ack "$2" {} -H ; }
bbfgcr() { var=$* ;
	find . -type f -name $1 | xargs grep -ni --color=auto ${var#*\ } ; }
bbvgcr() { vi $(grep -rni -P "$1" -l) ; }
bbvlc() { vi $(ls | grep "$1") ; }

# Useless bb
bbhmost() { cat ~/.zsh_history | awk '{sub(/^[^;]*;/,"",$0);print $0}' | \
	LC_ALL=C sort | uniq -c | LC_ALL=C sort -rn | head -n 20 ; }
bbhgc() { cat ~/.zsh_history | awk '{sub(/^[^;]*;/,"",$0);print $0}' | \
	grep -i "$1" | uniq -c | sort -n ; }

