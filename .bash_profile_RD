export PATH=/mtkoss/git:$PATH

alias ls=' ls'
alias cd=' cd'

alias ll='ls -alh --color'
alias cd.='cd ..'
alias cd..='cd ../..'
alias cd...='cd ../../..'
alias cd....='cd ../../../..'
unalias grep
alias gc='grep -i --color=auto'
alias gcr='grep -rni --color=auto'
alias gcre='grep -rni --color=auto -E'
alias gcrp='grep -rni --color=auto -P'
alias fdn='find . -name'
alias lc='ll | grep --color=auto'
alias lsc='ls | grep'
alias rebash='soruce ~/.bash_profile'
alias rezsh='soruce ~/.zshrc'
alias h='history'
alias p='ps -f'
alias upb='mtkbuild -o . -l -i'
alias upx='mtkbuild -o . -l -i -x'
alias upp='mtkbuild -o . -l -i -d -x'
alias duc='du -h --max-depth=1'
alias vs='emacs -nw'
alias vc='emacsclient -nw'
alias gg='git'
alias ggbi='git remote -v && git branch -a | head -n 10'
alias ggs='git status'
alias ggb='git branch -a'
alias ggc='git commit'
alias ggca='git commit --amend'
alias ggco='git checkout'
alias ggdf='git diff'
alias ggdfc='git diff --cached'
alias ggll='git ll'
alias ggl='git ls'
alias ggstash='git stash'

