bbpush() { git push mediatek HEAD:refs/for/$1 ; }
bbcaslb() { cas-labels -b alps-mp-$1 ; }
bbmeld() { meld $1/$3 $2/$3 ; }
bbup() { ls $* | xargs -I {} mtkbuild -o . -l -i -d -x {} ; }
bbv() { var=$*;file=${var%%:*};tmp=${var#*:};line=${tmp%%:*};vi_para=$file" +"$line;echo $vi_para; vi $vi_para ; }
bbfgcr() { var=$* ; find . -type f -name $1 | xargs grep -ni --color=auto ${var#*\ } ; }
bbvgcr() { vi $(gcrp "$1" -l) ; }

